{
 "manifest_version": 2,
	"name": "Remember good days",
	"description": "Tab app to show best moments from instagram",
    "version": "0.1",
    "background": {
      "scripts": ["js/vendor/angular.js"]
    },
   "content_security_policy": "script-src 'self' https://api.instagram.com; object-src 'self'",
   "chrome_url_overrides" : {
     "newtab": "tab.html"
   },
   "externally_connectable": {
    "matches": ["https://oauth.io/*"]
   }
}
