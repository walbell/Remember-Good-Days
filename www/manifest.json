{
 "manifest_version": 2,
	"name": "Remember good days - New Tab",
	"description": "Tab app to show best moments from Instagram",
    "version": "0.2",
    "background": {
      "scripts": ["js/vendor/angular.js"]
    },
   "content_security_policy": "script-src 'self' https://api.instagram.com https://cdn.mxpnl.com/libs/mixpanel-2-latest.min.js; object-src 'self'",
   "chrome_url_overrides" : {
     "newtab": "tab.html"
   },
   "externally_connectable": {
    "matches": ["https://oauth.io/*"]
   }
}
